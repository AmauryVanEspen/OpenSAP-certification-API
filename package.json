{
    "name": "devtoberfest",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^5",
        "@sap/hana-client": "^2.6.61",
        "express": "^4",
        "hdb": "^0.18.3",
        "@sap/audit-logging": "^5.0.0",
        "@sap/xsenv": "^3.1.0",
        "@sap/xssec": "^3.2.0",
        "passport": "^0.4.1"
    },
    "scripts": {
        "start": "cds serve gen/csn.json",
        "undeploy": "cf undeploy devtoberfest --delete-services --delete-service-keys"
    },
    "engines": {
        "node": "14.X"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "build": {
            "target": "."
        },
        "hana": {
            "deploy-format": "hdbtable"
        },
        "requires": {
            "db": {
                "kind": "hana"
            },
            "uaa": {
                "kind": "xsuaa"
            }
        }
    },
    "devDependencies": {
        "@sap/hdi-deploy": "4.2.0",
        "@sap/ux-specification": "^1.90.13"
    }
}
